{"ast":null,"code":"import { computed } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { Setting } from '@element-plus/icons-vue';\nimport auth from '../store/auth';\nexport default {\n  name: 'AppHeader',\n  components: {\n    Setting\n  },\n  setup() {\n    const router = useRouter();\n\n    // 获取用户名，优先显示用户名，如果没有则显示手机号\n    const userName = computed(() => {\n      if (!auth.state.isLoggedIn) return '';\n      return auth.state.user.username || auth.state.user.phone;\n    });\n\n    // 处理下拉菜单命令\n    const handleCommand = command => {\n      if (command === 'logout') {\n        auth.logout();\n        ElMessage.success('已退出登录');\n        router.push('/login');\n      }\n    };\n    return {\n      auth,\n      userName,\n      handleCommand\n    };\n  }\n};","map":{"version":3,"names":["computed","useRouter","ElMessage","Setting","auth","name","components","setup","router","userName","state","isLoggedIn","user","username","phone","handleCommand","command","logout","success","push"],"sources":["C:\\Users\\10150\\Desktop\\Hazardous-waste-management-system\\frontend\\src\\components\\AppHeader.vue"],"sourcesContent":["<template>\r\n  <div class=\"app-header\">\r\n    <div class=\"header-title\">\r\n      <h1>危险废物管理系统</h1>\r\n    </div>\r\n    <div class=\"user-info\" v-if=\"auth.state.isLoggedIn\">\r\n      <span class=\"welcome-text\">\r\n        欢迎,\r\n        <strong>{{ userName }}</strong>\r\n        <span class=\"role-tag\">({{ auth.state.user.role }})</span>\r\n        <span v-if=\"auth.state.user.unit_name\" class=\"unit-tag\">\r\n          {{ auth.state.user.unit_name }}\r\n        </span>\r\n      </span>\r\n      <el-dropdown @command=\"handleCommand\">\r\n        <span class=\"el-dropdown-link\">\r\n          <el-icon><setting /></el-icon>\r\n        </span>\r\n        <template #dropdown>\r\n          <el-dropdown-menu>\r\n            <el-dropdown-item command=\"logout\">退出登录</el-dropdown-item>\r\n          </el-dropdown-menu>\r\n        </template>\r\n      </el-dropdown>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { computed } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport { ElMessage } from 'element-plus';\r\nimport { Setting } from '@element-plus/icons-vue';\r\nimport auth from '../store/auth';\r\n\r\nexport default {\r\n  name: 'AppHeader',\r\n  components: {\r\n    Setting\r\n  },\r\n  setup() {\r\n    const router = useRouter();\r\n    \r\n    // 获取用户名，优先显示用户名，如果没有则显示手机号\r\n    const userName = computed(() => {\r\n      if (!auth.state.isLoggedIn) return '';\r\n      return auth.state.user.username || auth.state.user.phone;\r\n    });\r\n    \r\n    // 处理下拉菜单命令\r\n    const handleCommand = (command) => {\r\n      if (command === 'logout') {\r\n        auth.logout();\r\n        ElMessage.success('已退出登录');\r\n        router.push('/login');\r\n      }\r\n    };\r\n    \r\n    return {\r\n      auth,\r\n      userName,\r\n      handleCommand\r\n    };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.app-header {\r\n  background-color: #409EFF;\r\n  color: white;\r\n  padding: 0 20px;\r\n  height: 60px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.header-title h1 {\r\n  margin: 0;\r\n  font-size: 20px;\r\n}\r\n\r\n.user-info {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 20px;\r\n}\r\n\r\n.welcome-text {\r\n  font-size: 14px;\r\n}\r\n\r\n.role-tag, .unit-tag {\r\n  font-size: 12px;\r\n  margin-left: 5px;\r\n  padding: 2px 6px;\r\n  border-radius: 4px;\r\n  background-color: rgba(255, 255, 255, 0.3);\r\n}\r\n\r\n.el-dropdown-link {\r\n  cursor: pointer;\r\n  display: flex;\r\n  align-items: center;\r\n  color: white;\r\n}\r\n</style>\r\n"],"mappings":"AA6BA,SAASA,QAAO,QAAS,KAAK;AAC9B,SAASC,SAAQ,QAAS,YAAY;AACtC,SAASC,SAAQ,QAAS,cAAc;AACxC,SAASC,OAAM,QAAS,yBAAyB;AACjD,OAAOC,IAAG,MAAO,eAAe;AAEhC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,K