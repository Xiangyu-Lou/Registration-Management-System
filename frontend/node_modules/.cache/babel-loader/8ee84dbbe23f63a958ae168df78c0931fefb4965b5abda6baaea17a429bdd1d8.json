{"ast":null,"code":"import { computed } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { Setting } from '@element-plus/icons-vue';\nimport auth from '../store/auth';\nexport default {\n  name: 'AppHeader',\n  components: {\n    Setting\n  },\n  setup() {\n    const router = useRouter();\n\n    // 获取用户名，优先显示用户名，如果没有则显示手机号\n    const userName = computed(() => {\n      if (!auth.state.isLoggedIn) return '';\n      return auth.state.user.username || auth.state.user.phone;\n    });\n\n    // 处理下拉菜单命令\n    const handleCommand = command => {\n      if (command === 'logout') {\n        auth.logout();\n        ElMessage.success('已退出登录');\n        router.push('/login');\n      }\n    };\n    return {\n      auth,\n      userName,\n      handleCommand\n    };\n  }\n};","map":{"version":3,"names":["computed","useRouter","ElMessage","Setting","auth","name","components","setup","router","userName","state","isLoggedIn","user","username","phone","handleCommand","command","logout","success","push"],"sources":["C:\\Users\\10150\\Desktop\\hazardous-waste-management\\frontend\\src\\components\\AppHeader.vue"],"sourcesContent":["<template>\n  <div class=\"app-header\">\n    <div class=\"header-title\">\n      <h1>危险废物管理系统</h1>\n    </div>\n    <div class=\"user-info\" v-if=\"auth.state.isLoggedIn\">\n      <span class=\"welcome-text\">\n        欢迎,\n        <strong>{{ userName }}</strong>\n        <span class=\"role-tag\">({{ auth.state.user.role }})</span>\n        <span v-if=\"auth.state.user.unit_name\" class=\"unit-tag\">\n          {{ auth.state.user.unit_name }}\n        </span>\n      </span>\n      <el-dropdown @command=\"handleCommand\">\n        <span class=\"el-dropdown-link\">\n          <el-icon><setting /></el-icon>\n        </span>\n        <template #dropdown>\n          <el-dropdown-menu>\n            <el-dropdown-item command=\"logout\">退出登录</el-dropdown-item>\n          </el-dropdown-menu>\n        </template>\n      </el-dropdown>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { computed } from 'vue';\nimport { useRouter } from 'vue-router';\nimport { ElMessage } from 'element-plus';\nimport { Setting } from '@element-plus/icons-vue';\nimport auth from '../store/auth';\n\nexport default {\n  name: 'AppHeader',\n  components: {\n    Setting\n  },\n  setup() {\n    const router = useRouter();\n    \n    // 获取用户名，优先显示用户名，如果没有则显示手机号\n    const userName = computed(() => {\n      if (!auth.state.isLoggedIn) return '';\n      return auth.state.user.username || auth.state.user.phone;\n    });\n    \n    // 处理下拉菜单命令\n    const handleCommand = (command) => {\n      if (command === 'logout') {\n        auth.logout();\n        ElMessage.success('已退出登录');\n        router.push('/login');\n      }\n    };\n    \n    return {\n      auth,\n      userName,\n      handleCommand\n    };\n  }\n};\n</script>\n\n<style scoped>\n.app-header {\n  background-color: #409EFF;\n  color: white;\n  padding: 0 20px;\n  height: 60px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n}\n\n.header-title h1 {\n  margin: 0;\n  font-size: 20px;\n}\n\n.user-info {\n  display: flex;\n  align-items: center;\n  gap: 20px;\n}\n\n.welcome-text {\n  font-size: 14px;\n}\n\n.role-tag, .unit-tag {\n  font-size: 12px;\n  margin-left: 5px;\n  padding: 2px 6px;\n  border-radius: 4px;\n  background-color: rgba(255, 255, 255, 0.3);\n}\n\n.el-dropdown-link {\n  cursor: pointer;\n  display: flex;\n  align-items: center;\n  color: white;\n}\n</style>\n"],"mappings":"AA6BA,SAASA,QAAO,QAAS,KAAK;AAC9B,SAASC,SAAQ,QAAS,YAAY;AACtC,SAASC,SAAQ,QAAS,cAAc;AACxC,SAASC,OAAM,QAAS,yBAAyB;AACjD,OAAOC,IAAG,MAAO,eAAe;AAEhC,eAAe;EACbC,IAAI,EAAE,WAAW;EACjBC,UAAU,EAAE;IACVH;EACF,CAAC;EACDI,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIP,SAAS,CAAC,CAAC;;IAE1B;IACA,MAAMQ,QAAO,GAAIT,QAAQ,CAAC,MAAM;MAC9B,IAAI,CAACI,IAAI,CAACM,KAAK,CAACC,UAAU,EAAE,OAAO,EAAE;MACrC,OAAOP,IAAI,CAACM,KAAK,CAACE,IAAI,CAACC,QAAO,IAAKT,IAAI,CAACM,K